{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "components/*": ["./src/components/*"],
      "contexts/*": ["./src/contexts/*"],
      "hooks/*": ["./src/hooks/*"],
      "layout/*": ["./src/layout/*"],
      "utils/*": ["./src/utils/*"],
      "assets/*": ["./src/assets/*"],
      "pages/*": ["./src/pages/*"],
      "services/*": ["./src/services/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
